public String registerStudent(String name, String phone, String email) {
    
    // Creates and instantiate a list to store the record
    List<apxio__Student__c> students = new List<apxio__Student__c>();
    //apxio__Student__c students = apxio__Student__c>();

    // Add values to students
    students.add(new apxio__Student__c(Name = name, 
                                        apxio__Phone__c = phone, 
                                        apxio__Email__c = email));

    // Insert apxio__Student__c with name, phone and email
    insert students;

    // Query Registration Number based on student id
    apxio__Student__c registrationNumber = [SELECT apxio__Registration_Number__c 
                                            FROM apxio__Student__c 
                                            WHERE ID IN (:students.ID)];

    // Return the autogenerated apxio__Registration_Number__c
    return String.valueof(registrationNumber);
}