public Integer fibonacci(Integer n) {
    
    List<Integer> fib = new List<Integer>{1, 1};
    // 2 > 1
    if (n > 3) {

        for (Integer i = 2; i < n; i++){
            // currentSum += i;
            // fib[2] = 1 + 1
            fib[i] = fib[i-1] + fib[i-2];
        }
        return fib[n];
    } else {
        return 1;
    }

}